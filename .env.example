# ZetaChain Universal NFT Environment Configuration

# Solana Network Configuration
VITE_SOLANA_NETWORK=devnet
VITE_SOLANA_RPC_URL=https://api.devnet.solana.com
VITE_SOLANA_COMMITMENT=confirmed

# ZetaChain Network Configuration
VITE_ZETACHAIN_NETWORK=testnet
VITE_ZETACHAIN_RPC_URL=https://zetachain-testnet.rpc.com
VITE_ZETACHAIN_CHAIN_ID=7001

# Program IDs (Update with actual deployed program IDs)
VITE_UNIVERSAL_NFT_PROGRAM_ID=UniversalNFT1111111111111111111111111
VITE_ZETACHAIN_GATEWAY_PROGRAM_ID=ZetaGateway111111111111111111111111111

# Feature Flags
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_TESTNET=true
VITE_ENABLE_MAINNET=false

# Security Configuration
VITE_TSS_THRESHOLD=5
VITE_MIN_CONFIRMATIONS=3

# Application Configuration
VITE_APP_NAME="ZetaChain Universal NFT"
VITE_APP_VERSION=1.0.0
